<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>jt-manage</display-name>
  <!-- 为整个WEB应用的Servlet和JSP指定友
  	好错误提示页面 -->
  <error-page>
  	<error-code>500</error-code>
  	<location>/backend/500.jsp</location>
  </error-page>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ProdAddServlet</display-name>
    <servlet-name>ProdAddServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.ProdAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdAddServlet</servlet-name>
    <url-pattern>/ProdAddServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdListServlet</display-name>
    <servlet-name>ProdListServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.ProdListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdListServlet</servlet-name>
    <url-pattern>/ProdListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdDelServlet</display-name>
    <servlet-name>ProdDelServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.ProdDelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdDelServlet</servlet-name>
    <url-pattern>/ProdDelServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdInfoServlet</display-name>
    <servlet-name>ProdInfoServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.ProdInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdInfoServlet</servlet-name>
    <url-pattern>/ProdInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdUpdServlet</display-name>
    <servlet-name>ProdUpdServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.ProdUpdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdUpdServlet</servlet-name>
    <url-pattern>/ProdUpdServlet</url-pattern>
  </servlet-mapping>
</web-app>